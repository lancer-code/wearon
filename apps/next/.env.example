IGNORE_TS_CONFIG_PATHS=true
TAMAGUI_TARGET=web
TAMAGUI_DISABLE_WARN_DYNAMIC_LOAD=1

# Supabase Configuration (Public)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here

# Supabase Secret Key (Server-side only - DO NOT expose to client)
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# tRPC API URL
NEXT_PUBLIC_API_URL=http://localhost:3000/api/trpc

# Site URL (for OAuth callbacks)
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# OpenAI API (for GPT Image 1.5)
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MAX_RETRIES=3  # Set to 1 for dev, 3 for production

# Redis (for BullMQ job queue)
REDIS_URL=redis://your-redis-url-here

# Cron job secret (for Vercel)
CRON_SECRET=your-cron-secret-here
